	xmlns:mfp="http://www.ibm.com/mfp/integration"
	xmlns:http="http://www.ibm.com/mfp/integration/http">

    <displayName>UserProfile Adapter</displayName>
    <description>Adapter protected with socialLogin scope</description>

    <JAXRSApplicationClass>com.github.mfpdev.adapters.spring.integration.SpringXMLApplication</JAXRSApplicationClass>
    
    <property name="api_base_url" displayName="Api base url" defaultValue="https://api.us.apiconnect.ibmcloud.com/supermercados-cencosud-wong-development/dev" />
    <property name="api_client_id" displayName="Api client id to access api manager" defaultValue="5fcf4990-1035-4f93-ab3b-1fe385af6c6a" />
    <property name="api_secret" displayName="Api secret to access api manager" defaultValue="gX5nH3oH7fG1dG4qU2vS7kL5hJ5bS6uC3nY1fE6dI2pY6uR6hU" />  
    
    <property name="api_base_url_wong_v2" displayName="Api base url v2" defaultValue="https://api.us.apiconnect.ibmcloud.com/supermercados-cencosud-wong-development/wong-dev" />
    <property name="api_client_id_wong_v2" displayName="Api client id to access api manager v2" defaultValue="9980e9d4-5fda-4164-bbbe-469a12fc5471" />
    <property name="api_secret_wong_v2" displayName="Api secret to access api manager v2" defaultValue="aG1sA4sS4eO2qP6mJ5iA7oX8tN3kC3fY5bD6tI7wL2xN8vP4eB" />  

</mfp:adapter>
